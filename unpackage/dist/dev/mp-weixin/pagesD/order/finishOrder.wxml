<view><view data-event-opts="{{[['tap',[['choseAddress',['$event']]]]]}}" class="address-section" bindtap="__e"><view class="order-content"><text class="iconfont icon-dizhi01"></text><block wx:if="{{addrId!=''}}"><view class="cen"><view class="top"><text class="name">{{"收货人:"+addressData.name}}</text><text class="mobile">{{addressData.phone}}</text></view><text class="address">{{addressData.address}}</text></view></block><block wx:else><view class="cen">请选择收货地址</view></block><view class="iconfont icon-zuojiantou"></view></view></view><view class="goods-section"><block wx:for="{{dataList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view class="g-item"><block wx:if="{{v.image}}"><image class="product-img" src="{{v.image}}"></image></block><block wx:else><image class="product-img" src="{{v.icon}}"></image></block><view class="right"><text class="title clamp">{{v.name}}</text><block wx:if="{{v.sku}}"><text class="spec">{{"规格: "+v.sku}}</text></block><block wx:else><text class="spec">规格:<block wx:for="{{skuList}}" wx:for-item="i" wx:for-index="j" wx:key="j"><block wx:if="{{payType==0}}"><text>{{i.value}}</text></block></block></text></block></view><view class="price-box"><text class="price">{{"￥"+v.price}}</text><text class="number">{{"x "+v.count}}</text></view></view></block></view><view class="yt-list"><block wx:if="{{type!=2}}"><view class="yt-list-cell b-b"><text class="cell-tit clamp">{{"共计"+totalCount+"件商品"}}</text><block wx:if="{{shopPrice!=0}}"><text class="cell-tip">{{"商品金额 ￥"+shopPrice}}</text></block></view></block><block wx:if="{{type==0}}"><view data-event-opts="{{[['tap',[['gotoChooseCompline',['$event']]]]]}}" class="yt-list-cell b-b" bindtap="__e"><text class="cell-tit clamp">优惠劵</text><view class="cell-tip uni-flex">{{"-￥"+complinNumber+''}}<view class="iconfont icon-zuojiantou" style="color:#CCCCCC;font-size:20px;"></view></view></view></block><view class="yt-list-cell b-b"><view data-event-opts="{{[['tap',[['interShow',[7]]]]]}}" class="cell-tit icon-swiper" bindtap="__e"><view class="clamp">运费</view><image class="tishiimg" src="/static/image/tishi@2x.png" mode></image></view><text class="cell-tip">{{"￥"+freight}}</text></view><view class="yt-list-cell b-b"><view data-event-opts="{{[['tap',[['interShow',[8]]]]]}}" class="cell-tit icon-swiper" bindtap="__e"><view class=" clamp">税务</view><image class="tishiimg" src="/static/image/tishi@2x.png" mode></image></view><text class="cell-tip">{{"￥"+taxes}}</text></view><block wx:if="{{type!=2}}"><view class="yt-list-cell b-b"><view class="cell-tit clamp">订单总额</view><text class="cell-tip">{{"￥"+endPrice}}</text></view></block></view><view class="yt-list" style="margin-top:10px;"><block wx:if="{{type!=2}}"><view class="yt-list-cell b-b"><view class="cell-tit clamp">获得积分</view><text class="cell-tip">{{''+totalPoint+''}}</text></view></block></view><view class="{{['shuoming-con',(type!=0)?'pos':'']}}">说明<rich-text nodes="{{finishedContent}}"></rich-text></view><view class="footer"><view class="price-content"><text>实付款</text><block wx:if="{{endPrice!=0}}"><text class="price-tip">￥</text></block><text class="price">{{endPrice}}</text></view><text class="submit" hover-class="btn-hover" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交订单</text></view><block wx:if="{{showPopup}}"><uni-popup class="uni-popup" vue-id="1" type="middle" data-event-opts="{{[['^click',[['e0']]]]}}" catch:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-popup__wrapper-box"><view class="uni-puop-content"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="iconfont icon-error" style="color:#CCCCCC;" catchtap="__e"></view><view class="line"></view><view class="uni-puop-info"><view class="puop-title">{{''+puopTitle+''}}</view><rich-text nodes="{{content}}"></rich-text></view></view></view></uni-popup></block></view>