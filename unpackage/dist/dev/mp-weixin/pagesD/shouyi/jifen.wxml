<view class="content"><view class="jifen-top"><text style="font-size:30px;">{{integral}}</text><text style="font-size:15px;">我的积分</text></view><view class="{{['navbar',(showPos)?'pos':'']}}"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tabClick',[index]]]]]}}" class="{{['nav-item',(tabCurrentIndex==index)?'current':'']}}" bindtap="__e">{{''+item.text+''}}</view></block></view><swiper class="swiper-box" duration="300" current="{{tabCurrentIndex}}" data-event-opts="{{[['change',[['changeTarba',['$event']]]]]}}" bindchange="__e"><block wx:for="{{navList}}" wx:for-item="tabItem" wx:for-index="tabIndex" wx:key="tabIndex"><swiper-item class="tab-content"><scroll-view class="list-scroll-content" scroll-y data-event-opts="{{[['scrolltolower',[['loadDataMore',['$event']]]]]}}" bindscrolltolower="__e"><block wx:if="{{tabItem.dataList.length==0&&tabItem.load}}"><empty vue-id="{{'1-'+tabIndex}}" bind:__l="__l"></empty></block><block wx:else><block wx:for="{{tabItem.dataList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view class="list-item"><view class="list-item-left"><view class="list-top">{{''+v.title+''}}</view><view class="list-bottom">{{''+v.adtime+''}}</view></view><block wx:if="{{v.szType==0}}"><view class="list-item-right">{{'-'+v.integral+''}}</view></block><block wx:if="{{v.szType==1}}"><view class="list-item-right">{{'+'+v.integral+''}}</view></block></view></block></block><block wx:if="{{tabItem.dataList.length!=0}}"><uni-load-more vue-id="{{'2-'+tabIndex}}" status="{{tabItem.status}}" bind:__l="__l"></uni-load-more></block></scroll-view></swiper-item></block></swiper></view>