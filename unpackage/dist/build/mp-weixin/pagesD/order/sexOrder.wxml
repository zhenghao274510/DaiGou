<view><view data-event-opts="{{[['tap',[['choseAddress',['$event']]]]]}}" class="address-section" bindtap="__e"><view class="order-content"><text class="iconfont icon-dizhi01"></text><block wx:if="{{addrId!=''}}"><view class="cen"><view class="top"><text class="name">{{addressData.name}}</text><text class="mobile">{{addressData.phone}}</text></view><text class="address">{{addressData.province+addressData.city+" "+addressData.area+addressData.details}}</text></view></block><block wx:else><view class="cen">请选择收货地址</view></block><view class="iconfont icon-zuojiantou"></view></view></view><view class="goods-section"><view class="g-item"><image class="product-img" src="{{dataObj.lbImageList[0]}}"></image><view class="right"><text class="title clamp">{{dataObj.name}}</text></view><view class="price-box"><text class="price">{{"￥"+dataObj.price}}</text><text class="number">{{"x "+count}}</text></view></view></view><view class="yt-list"><block wx:if="{{type!=2}}"><view class="yt-list-cell b-b"><text class="cell-tit clamp">{{"共计"+count+"件商品"}}</text><text class="cell-tip">{{"商品金额 ￥"+shopPrice}}</text></view></block><view class="yt-list-cell b-b"><view data-event-opts="{{[['tap',[['interShow',[7]]]]]}}" class="cell-tit icon-swiper" bindtap="__e"><view class="clamp">运费</view><image class="tishiimg" src="/static/image/tishi@2x.png" mode></image></view><text class="cell-tip">{{"￥"+freight}}</text></view><view class="yt-list-cell b-b"><view data-event-opts="{{[['tap',[['interShow',[8]]]]]}}" class="cell-tit icon-swiper" bindtap="__e"><view class=" clamp">税务</view><image class="tishiimg" src="/static/image/tishi@2x.png" mode></image></view><text class="cell-tip">{{"￥"+taxes}}</text></view><block wx:if="{{type!=2}}"><view class="yt-list-cell b-b"><view class="cell-tit clamp">订单总额</view><text class="cell-tip">{{price}}</text></view></block></view><view class="yt-list" style="margin-top:10px;"><block wx:if="{{type==2}}"><view class="yt-list-cell b-b"><view class="cell-tit clamp">所需积分</view><text class="cell-tip">{{dataObj.point}}</text></view></block><block wx:if="{{type==1}}"><view class="yt-list-cell b-b"><view class="cell-tit clamp">获得积分</view><text class="cell-tip">{{''+dataObj.point+''}}</text></view></block></view><view class="shuoming-con pos">说明</view><view class="footer"><view class="price-content"><text>实付款</text><text class="price-tip">￥</text><text class="price">{{price}}</text></view><text class="submit" hover-class="btn-hover" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交订单</text></view><vue-pwd vue-id="1" dispay="{{showPwdBox}}" data-event-opts="{{[['^getData',[['subOrder']]]]}}" bind:getData="__e" bind:__l="__l"></vue-pwd><block wx:if="{{showPopup}}"><uni-popup class="uni-popup" vue-id="2" type="middle" data-event-opts="{{[['^click',[['e0']]]]}}" catch:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-popup__wrapper-box"><view class="uni-puop-content"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="iconfont icon-error" style="color:#CCCCCC;" catchtap="__e"></view><view class="line"></view><view class="uni-puop-info"><view class="puop-title">{{''+puopTitle+''}}</view><rich-text nodes="{{content}}"></rich-text></view></view></view></uni-popup></block></view>